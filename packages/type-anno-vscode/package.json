{
  "name": "type-anno-vscode",
  "displayName": "JavaScript Type Annotation",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "publisher": "oneofthezombies",
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "JavaScript Type Annotation",
      "properties": {
        "type-anno.arguments": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "Arguments of type-anno CLI tools"
        }
      }
    }
  },
  "type": "commonjs",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "dependencies": {
    "@oneofthezombies/type-anno": "*",
    "ts-morph": "24.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.94.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1"
  }
}
